{
    "name": "pet_tracker",
    "version": "1.0.0",
    "main": "index.ts",
    "scripts": {
        "test": "vitest ./tests/",
        "swagger": "node swagger.cjs",
        "swagger-build": "node ./dist/swagger.cjs",
        "dev": "nodemon",
        "start": "node index.js",
        "build": "del-cli ./dist && tsc && copyfiles ./swagger.cjs ./dist && copyfiles -f ./production/* ./dist && copyfiles -f ./package*.json ./dist"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "description": "",
    "dependencies": {
        "@types/jsonwebtoken": "^9.0.10",
        "axios": "^1.11.0",
        "cookie-parser": "^1.4.7",
        "cors": "^2.8.5",
        "dotenv": "^17.0.1",
        "express": "^5.1.0",
        "express-validator": "^7.2.1",
        "google-auth-library": "^10.1.0",
        "googleapis": "^150.0.1",
        "jsonwebtoken": "^9.0.2",
        "mongoose": "^8.16.1",
        "multer": "^2.0.1",
        "swagger-autogen": "^2.23.7",
        "swagger-ui-express": "^5.0.1",
        "ts-node": "^10.9.2",
        "typescript": "^5.8.3"
    },
    "nodemonConfig": {
        "watch": [
            "."
        ],
        "ext": "ts",
        "exec": "ts-node . --project tsconfig.json",
        "legacyWatch": true,
        "events": {
            "restart": "npm run swagger"
        }
    },
    "devDependencies": {
        "@types/axios": "^0.9.36",
        "@types/cookie-parser": "^1.4.9",
        "@types/cors": "^2.8.19",
        "@types/express": "^5.0.3",
        "@types/mongoose": "^5.11.96",
        "@types/multer": "^2.0.0",
        "@types/node": "^24.0.10",
        "@types/swagger-ui-express": "^4.1.8",
        "copyfiles": "^2.4.1",
        "del-cli": "^6.0.0",
        "nodemon": "^3.1.10",
        "rollup": "^4.0.0",
        "vitest": "^3.2.4"
    }
}
